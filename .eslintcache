...potlucks,\n      [e.target.name]: e.target.value,\n    });\n  };\n  const newPotluck = {\n    id: Date.now(),\n    name: potlucks.eventTitle,\n    firstName: potlucks.firstName,\n    lastName: potlucks.lastName,\n    location: potlucks.location,\n    time: potlucks.time,\n    date: potlucks.date,\n  };\n  const handleSubmit = (e) => {\n    e.preventDefault();\n    const newPotluck = {\n      id: Date.now(),\n      name: potlucks.eventTitle,\n      firstName: potlucks.firstName,\n      lastName: potlucks.lastName,\n      location: potlucks.location,\n      time: potlucks.time,\n      date: potlucks.date,\n    };\n    axiosWithAuth()\n      .post(`/potluck`, newPotluck)\n      .then((res) => {\n        console.log(`Post New Event Success: ${res.data}`);\n        setPotlucks(initialValues);\n      })\n      .catch((err) => {\n        console.log(`Error on POST for new Event ${err}`);\n        console.log(\"submitted data: \", newPotluck);\n      });\n    //setEvent([...event, newPotluck]);\n  };\n  setPotlucks([...potlucks, newPotluck]);\n  return (\n    <Form onSubmit={handleSubmit}>\n      <FormGroup>\n        <Label for=\"eventTitle\">Potluck Name</Label>\n        <Input\n          type=\"text\"\n          name=\"eventTitle\"\n          value={potlucks.eventTitle}\n          onChange={handleChange}\n          required\n        />\n      </FormGroup>\n      <FormGroup>\n        <Label for=\"firstName\">Your First Name</Label>\n        <Input\n          type=\"text\"\n          name=\"firstName\"\n          id=\"firstName\"\n          value={potlucks.firstName}\n          onChange={handleChange}\n          required\n        />\n      </FormGroup>\n      <FormGroup>\n        <Label for=\"lastName\">Your Last Name</Label>\n        <Input\n          type=\"text\"\n          name=\"lastName\"\n          value={potlucks.lastName}\n          onChange={handleChange}\n          required\n        />\n      </FormGroup>\n      <FormGroup>\n        <Label for=\"location\">Location of the Event</Label>\n        <Input\n          type=\"text\"\n          name=\"location\"\n          value={potlucks.location}\n          onChange={handleChange}\n          required\n        />\n      </FormGroup>\n      <FormGroup>\n        <Label for=\"date\">Day of Event</Label>\n        <Input\n          type=\"date\"\n          name=\"date\"\n          value={potlucks.date}\n          onChange={handleChange}\n          required\n        />\n      </FormGroup>\n      <FormGroup>\n        <Label for=\"time\">Time of Event</Label>\n        <Input\n          type=\"time\"\n          name=\"time\"\n          value={potlucks.time}\n          onChange={handleChange}\n          required\n        />\n      </FormGroup>\n      <Button type=\"submit\">Submit</Button>\n    </Form>\n  );\n};\nexport default NewEventForm;\n","/Users/eli/MyRepos/UNIT_3/bweek/frontend/src/components/Potlucks/EventCard.js",["82"],"import React, { useEffect, setState } from \"react\";\nimport axios from \"axios\";\nimport { Card, CardHeader, CardTitle, CardSubtitle } from \"reactstrap\";\n\nexport default function EventCard(props) {\n  const { potlucks, setPotlucks } = props;\n  console.log(potlucks, \"Event Card data\");\n  useEffect(() => {\n    axios\n      .get(\"https://tt-webpt-92-potluck-app.herokuapp.com/potluck\")\n      .then((res) => {\n        setPotlucks(res.data);\n        console.log(res.data);\n      })\n      .catch((err) => {\n        console.log(err, `Event Card GET error potlucks`);\n      });\n  });\n\n  const { foodList, setFoodList } = setState([]);\n  useEffect(() => {\n    axios\n      .get(\"https://tt-webpt-92-potluck-app.herokuapp.com/food\")\n      .then((res) => {\n        setFoodList(res.data);\n      })\n      .catch((err) => {\n        console.log(err, `EventCard error GET foodList`);\n      });\n  });\n\n  return (\n    <div>\n      {potlucks.map((potluck) => {\n        return (\n          <Card key={potluck.id}>\n            <CardHeader>{potluck.eventName}</CardHeader>\n            <CardTitle>Location: {potluck.location}</CardTitle>\n            <CardSubtitle>Date: {potluck.date}</CardSubtitle>\n            <CardSubtitle>Time: {potluck.time}</CardSubtitle>\n            <CardSubtitle>\n              Host Name: {potluck.firstName} {potluck.lastName}\n            </CardSubtitle>\n            <CardSubtitle>Food to Bring:</CardSubtitle>\n            <ul>\n              {foodList.map((item) => {\n                <li>{item.name}</li>;\n              })}\n            </ul>\n            <h4>\n              <a href=\"mailto:email@email.com\">RSVP</a>\n            </h4>\n          </Card>\n        );\n      })}\n      {/*Delete button? Do we want this?\n        <button onClick = (e) => {\n          e.stopPropagation()\n          e.preventDefault()\n          this.triggerDelete(permit);\n          deleteEvent(potluck.id)>Delete this Event</button>\n        */}\n    </div>\n  );\n}\n",{"ruleId":"83","replacedBy":"84"},{"ruleId":"85","replacedBy":"86"},{"ruleId":"87","severity":1,"message":"88","line":1,"column":27,"nodeType":"89","messageId":"90","endLine":1,"endColumn":36},{"ruleId":"87","severity":1,"message":"91","line":3,"column":8,"nodeType":"89","messageId":"90","endLine":3,"endColumn":17},{"ruleId":"87","severity":1,"message":"92","line":4,"column":8,"nodeType":"89","messageId":"90","endLine":4,"endColumn":21},{"ruleId":"87","severity":1,"message":"93","line":6,"column":8,"nodeType":"89","messageId":"90","endLine":6,"endColumn":20},{"ruleId":"87","severity":1,"message":"94","line":7,"column":8,"nodeType":"89","messageId":"90","endLine":7,"endColumn":19},{"ruleId":"87","severity":1,"message":"95","line":9,"column":8,"nodeType":"89","messageId":"90","endLine":9,"endColumn":13},{"ruleId":"87","severity":1,"message":"96","line":25,"column":10,"nodeType":"89","messageId":"90","endLine":25,"endColumn":16},{"ruleId":"87","severity":1,"message":"97","line":27,"column":9,"nodeType":"89","messageId":"90","endLine":27,"endColumn":1